eclipse.preferences.version=1
gdb.client.commands=set mem inaccessible-by-default off
gdb.client.doStart=true
gdb.client.executable=${cy_sdk_install_dir}/tools/gcc-7.2.1-1.0/bin/arm-none-eabi-gdb${cy_exe_platform_ext}
gdb.client.other=
gdb.openocd.doDebugInRam=false
gdb.openocd.doInitialReset=false
gdb.openocd.doPreRunReset=true
gdb.openocd.enableSemihosting=true
gdb.openocd.init.other=
gdb.openocd.initialReset.type=init
gdb.openocd.preRun.other=mon psoc6 reset_halt sysresetreq
gdb.openocd.preRunReset.type=run
gdb.server.doStart=true
gdb.server.executable=${cy_tools_path\:openocd}/bin/openocd
gdb.server.other=-s "${cy_tools_path\:openocd}/scripts"\r\n-s "${workspace_loc}/OnethinxCore_00_config/GeneratedSource"\r\n-c "source [find interface/kitprog3.cfg]"\r\n-c "transport select swd"\r\n-c "set ENABLE_CM0 0"\r\n-c "set ENABLE_AQUIRE  0"\r\n-c "source [find target/psoc6.cfg]"\r\n-c "puts stderr {Started by GNU MCU Eclipse}"\r\n-c "init; reset init"
